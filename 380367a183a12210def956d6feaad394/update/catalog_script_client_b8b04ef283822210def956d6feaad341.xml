        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }

    var dateNeeded = g_form.getValue('date_needed');
    var dateToReturn = g_form.getValue('date_to_return');

    var needed = new Date(dateNeeded.split(" ")[0]);
    var toReturn = new Date(dateToReturn.split(" ")[0]);

    if (isNaN(needed.getTime()) || isNaN(toReturn.getTime())) {
        g_form.addErrorMessage("One or both dates are invalid.");
        return;
    }

    if (toReturn.getTime() < needed.getTime()) {
        g_form.addErrorMessage('Date to return should not be before the Date needed.');
        g_form.setValue('date_to_return', '');
    }
}
]]></script>
