        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
  if (isLoading || newValue == '') {
    return;
  }

  // GlideAjax call to get the vehicle info
  var ga = new GlideAjax('GetVehicleDetails');
  ga.addParam('sysparm_name', 'getDetails');
  ga.addParam('sysparm_sys_id', newValue);
  ga.getXMLAnswer(function(response) {
    var data = JSON.parse(response);

    var html =
      '<div style="background:#f3f3f3; padding:10px; border-radius:8px;">' +
      '<strong>Vehicle Info</strong><br>' +
      'Make: ' + data.vehicle_make + '<br>' +
      'Model: ' + data.vehicle_model + '<br>' +
      'Year: ' + data.vehicle_year +
      '</div>';

    g_form.setValue('vehicle_display_card', html);
  });
}

]]></script>
