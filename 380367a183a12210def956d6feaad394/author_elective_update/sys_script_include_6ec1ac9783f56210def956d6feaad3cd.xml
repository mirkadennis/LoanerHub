        <api_name>x_1474716_lva.GetVehicleDetails</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description/>
        <name>GetVehicleDetails</name>
        <script><![CDATA[var GetVehicleDetails = Class.create();
GetVehicleDetails.prototype = Object.extendsObject(AbstractAjaxProcessor, {

  getDetails: function() {
    var vehicleSysId = this.getParameter('sysparm_vehicle_sys_id');
    var vehicleGR = new GlideRecord('x_1474716_lva_vehicle_loaner_catalog');
    
    if (vehicleGR.get(vehicleSysId)) {
      var make = vehicleGR.getValue('vehicle_make');
      var model = vehicleGR.getValue('vehicle_model');
      var year = vehicleGR.getValue('vehicle_year');

      return 'Make: ' + make + '<br>Model: ' + model + '<br>Year: ' + year;
    }
    
    return 'Vehicle details not found.';
  }

});
]]></script>
